  <DataSets>
    <DataSet Name="dsNetRevenueQuantity">
      <Query>
        <DataSourceName>ContosolDW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@prmYear">
            <Value>=Parameters!prmYear.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT S.ProductKey, P.ProductName, SUM(S.SalesAmount - S.ReturnAmount - S.DiscountAmount) AS Net_Revenue, 
                 SUM( S.SalesQuantity - S.ReturnQuantity ) AS Net_Quantity
FROM     FactSales AS S INNER JOIN
                  DimProduct AS P ON S.ProductKey = P.ProductKey INNER JOIN
                  DimDate AS D ON S.DateKey = D.Datekey INNER JOIN
                  DimCurrency AS C ON S.CurrencyKey = C.CurrencyKey
WHERE  (S.CurrencyKey = 1) AND (D.CalendarYear IN (@prmYear))
GROUP BY S.ProductKey, P.ProductName
ORDER BY Net_Revenue DESC, Net_Quantity DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductKey">
          <DataField>ProductKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Net_Revenue">
          <DataField>Net_Revenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Net_Quantity">
          <DataField>Net_Quantity</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!Net_Revenue.Value</FilterExpression>
          <Operator>TopN</Operator>
          <FilterValues>
            <FilterValue>=Parameters!prmRowNumber.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
  </DataSets>
