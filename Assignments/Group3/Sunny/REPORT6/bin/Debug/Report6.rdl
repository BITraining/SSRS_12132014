    <DataSet Name="StoreDetail">
      <Query>
        <DataSourceName>STOREDETAIL</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@STOREID">
            <Value>=Parameters!STOREID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@country">
            <Value>=Parameters!country.Value</Value>
