  <DataSets>
    <DataSet Name="BestSellingProduct">
      <Query>
        <DataSourceName>Contoso</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="">
            <Value>=Parameters!TopN.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        DimProduct.ProductName, CAST(SUM(FactSales.SalesAmount - FactSales.ReturnAmount - FactSales.DiscountAmount) AS decimal(12, 2)) AS NetRevenue
FROM            DimProduct INNER JOIN
                         FactSales ON DimProduct.ProductKey = FactSales.ProductKey INNER JOIN
                         DimStore AS DimStore_1 ON FactSales.StoreKey = DimStore_1.StoreKey INNER JOIN
                         DimGeography AS DimGeography_1 ON DimStore_1.GeographyKey = DimGeography_1.GeographyKey INNER JOIN
                         DimSalesTerritory ON DimGeography_1.GeographyKey = DimSalesTerritory.GeographyKey
GROUP BY DimProduct.ProductName
ORDER BY NetRevenue DESC, DimProduct.ProductName</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NetRevenue">
          <DataField>NetRevenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!NetRevenue.Value</FilterExpression>
          <Operator>TopN</Operator>
          <FilterValues>
            <FilterValue>=Parameters!TopN.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
  </DataSets>
