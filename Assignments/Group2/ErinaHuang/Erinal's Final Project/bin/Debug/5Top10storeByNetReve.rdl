    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Top10StoreByNetReve">
      <Query>
        <DataSourceName>Constoso</DataSourceName>
        <CommandText>SELECT        DimDate.CalendarYear, DimProduct.ProductName, SUM(FactSales.SalesQuantity - FactSales.DiscountQuantity - FactSales.ReturnQuantity) AS NetQuantity, 
                         SUM(FactSales.SalesAmount - FactSales.DiscountAmount - FactSales.ReturnAmount) AS NetRevenue
FROM            FactSales INNER JOIN
                         DimDate ON FactSales.DateKey = DimDate.Datekey INNER JOIN
                         DimProduct ON DimProduct.ProductKey = FactSales.ProductKey
GROUP BY DimDate.CalendarYear, DimProduct.ProductName</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductName">
          <DataField>ProductName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NetQuantity">
          <DataField>NetQuantity</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CalendarYear">
          <DataField>CalendarYear</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NetRevenue">
          <DataField>NetRevenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!Revenue.Value</FilterExpression>
          <Operator>TopN</Operator>
          <FilterValues>
            <FilterValue>=Parameters!TopN.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="TopN">
      <DataType>Integer</DataType>
      <DefaultValue>
        <Values>
          <Value>10</Value>
        </Values>
